{
    "provider": {
        "ip_range": "172.16.0.0/24",
        "loopback_range": "192.168.0.0/16",
        "BGP_asn": 65000,
        "route_reflector": "P1",
        "routers": {
            "PE1": {
                "interfaces": {
                    "g1/0": ["P1"],
                    "g2/0": ["CE1A"],
                    "g3/0": ["CE2A"]
                }
            },
            "PE2": {
                "interfaces": {
                    "g1/0": ["P2"],
                    "g2/0": ["CE1B"],
                    "g3/0": ["CE2B"]
                }
            },
            "P1": {
                "interfaces": {
                    "g1/0": ["PE1"],
                    "g2/0": ["P2"]
                }
            },
            "P2": {
                "interfaces": {
                    "g1/0": ["PE2"],
                    "g2/0": ["P1"]
                }
            }
        }
    },
    "clients": {
        "global": {
            "ip_range": "172.20.1.0/24"
        },
        "rouge": {
            "routers": {
                "CE1A": {
                    "BGP_asn": 100,
                    "BGP_advertized_networks": [
                        "192.168.1.0/24"
                    ],
                    "interfaces": {
                        "g1/0": "PE1"
                    },
                    "unmanaged_interfaces": {
                        "g2/0": "192.168.1.1/24"
                    }
                },
                "CE1B": {
                    "BGP_asn": 200,
                    "BGP_advertized_networks": [
                        "192.168.2.0/24"
                    ],
                    "interfaces": {
                        "g1/0": "PE2"
                    },
                    "unmanaged_interfaces": {
                        "g2/0": "192.168.2.1/24"
                    }
                }
            }
        },
        "vert": {
            "routers": {
                "CE2A": {
                    "BGP_asn": 300,
                    "BGP_advertized_networks": [
                        "192.168.1.0/24"
                    ],
                    "interfaces": {
                        "g1/0": "PE1"
                    },
                    "unmanaged_interfaces": {
                        "g2/0": "192.168.1.1/24"
                    }
                },
                "CE2B": {
                    "BGP_asn": 400,
                    "BGP_advertized_networks": [
                        "192.168.2.0/24"
                    ],
                    "interfaces": {
                        "g1/0": "PE2"
                    },
                    "unmanaged_interfaces": {
                        "g2/0": "192.168.2.1/24"
                    }
                }
            }
        }
    }
}